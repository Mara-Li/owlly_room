def emojis_random():
    all_emojis = emojis.db.get_emoji_aliases()
    all_emojis_key=list(all_emojis.keys())
    decodation=[]
    for i in range (0, len(all_emojis_key)):
        d=emojis.encode(all_emojis_key[i])
        decodation.append(d)
    rand_emoji=random.sample(decodation,1)
    rand_emoji = random[0]
    return rand_emoji